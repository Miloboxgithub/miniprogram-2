.container{
  margin-top: 200rpx;
}
.one{
  display: flex;
  justify-content: space-between;
  padding: 10rpx;
}
.place{
  display: flex;
  justify-content: center;
  align-items: center;
}
.place image{
  width: 40rpx;
  height: 40rpx;
}
/* two */
.two{
  margin-top: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
}
.two image{
  width: 400rpx;
  height: 400rpx;
}
.pp{
  font-size: 20px;
  margin-bottom: 20px;
}
.p{
  margin-bottom: 10px;
  font-size: 25px;
  display: flex;
  justify-content: center;
  align-items: center;
}
/* three */
.three{
  margin-top: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
}
.t{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-bottom: 20rpx;
}
.tt{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin-top: 30px;
  font-size: 20px;
}
.container2 {
  background-color: rgb(255, 255, 255);
  padding: 5px;
  margin: 10px 10px 10px 10px;
  border: 1px solid #c9c6c6;
  box-shadow: 2px 4px 4px #7a7a7a;
  border-radius: 10px;
height: 700px;
}